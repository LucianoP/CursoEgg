Scanner sc = new Scanner(System.in);        boolean result;        System.out.println("Ingrese un número");        int num = sc.nextInt();                result = primo(num);                System.out.println("El número "+num+" es primo? "+result);          }    public static boolean primo(int num){        int cont = 0;        boolean resultado;        for (int i = 1; i <= num; i++) {            if (num % i == 0) {                cont++;            }        }        resultado = cont==2;        return resultado;    }    }